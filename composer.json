{
    "name": "genealogiawebsite/searchable",
    "description": "Searchable dependency for Laravel Enso",
    "keywords": [
        "genealogiawebsite",
        "searchable"
    ],
    "homepage": "https://github.com/genealogiawebsite/searchable",
    "type": "library",
    "license": "MIT",
    "authors": [{
        "name": "Adrian Ocneanu",
        "email": "aocneanu@gmail.com",
        "homepage": "https://genealogiawebsite.com",
        "role": "Developer"
    }],
    "require": {
        "genealogiawebsite/core": "^6.0",
        "genealogiawebsite/filters": "^2.0",
        "genealogiawebsite/migrator": "^2.0",
        "genealogiawebsite/permissions": "^4.1"
    },
    "autoload": {
        "psr-4": {
            "LaravelEnso\\Searchable\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "LaravelEnso\\Searchable\\AppServiceProvider",
                "LaravelEnso\\Searchable\\SearchServiceProvider"
            ],
            "aliases": {
                "Search": "LaravelEnso\\Searchable\\Facades\\Search"
            }
        }
    }
}
